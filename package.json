{
  "name": "bulk-mongo",
  "version": "0.0.7",
  "description": "Writable object stream using MongoDb bulk mode",
  "main": "index.js",
  "engines": {
    "node": ">= 0.10.7"
  },
  "scripts": {
    "test": "mocha --check-leaks --reporter spec --bail test/[^p]*.js",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --check-leaks --reporter dot test/[^p]*.js",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --check-leaks --reporter spec test/[^p]*.js",
    "test-perf": "mocha --bail --timeout 120000 test/perf*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/villu357/bulk-mongo.git"
  },
  "keywords": [
    "mongodb",
    "stream",
    "writable"
  ],
  "author": "Villem Alango",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/villu357/bulk-mongo/issues"
  },
  "homepage": "https://github.com/villu357/bulk-mongo",
  "dependencies": {
    "debug": "^2.1.0",
    "mongodb": "^1.4.20",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "istanbul": "^0.3.2",
    "mocha": "^2.0.1",
    "should": "^4.3.0",
    "sinon": "^1.12.1"
  }
}
